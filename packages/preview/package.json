{
  "name": "preview",
  "version": "1.3.4",
  "private": true,
  "description": "Preview component for obsidian ember plugin",
  "repository": "",
  "license": "MIT",
  "author": "mrloop",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "vite",
    "test": "concurrently \"pnpm:lint\" \"pnpm:test:*\" --names \"lint,test:\" --prefixColors auto",
    "test:ember": "vite build --mode test && ember test --path dist"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/eslint-parser": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/runtime": "^7.0.0",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^4.0.0",
    "@ember/test-helpers": "^4.0.5",
    "@embroider/compat": "^4.0.0-alpha.0",
    "@embroider/config-meta-loader": "^1.0.0-alpha.0",
    "@embroider/core": "^4.0.0-alpha.0",
    "@embroider/macros": "^1.17.2",
    "@embroider/router": "^3.0.1",
    "@embroider/vite": "^1.0.0-alpha.0",
    "@eslint/js": "^9.25.1",
    "@glimmer/compiler": "^0.94.10",
    "@glimmer/component": "^2.0.0",
    "@glimmer/syntax": "^0.94.9",
    "@glimmer/tracking": "^1.1.2",
    "@glint/template": "^1.5.2",
    "@rollup/plugin-babel": "^6.0.4",
    "babel-plugin-ember-template-compilation": "^2.4.1",
    "concurrently": "^9.1.2",
    "content-tag": "^4.0.0",
    "decorator-transforms": "^2.3.0",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~6.4.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-deprecation-workflow": "^3.3.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.0",
    "ember-qunit": "^9.0.2",
    "ember-repl": "^6.0.0",
    "ember-resolver": "^13.1.0",
    "ember-resources": "^7.0.4",
    "ember-source": "~6.4.0",
    "ember-template-imports": "^4.3.0",
    "ember-template-lint": "^6.1.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-qunit": "^8.1.2",
    "globals": "^15.15.0",
    "prettier": "^3.5.3",
    "prettier-plugin-ember-template-tag": "^2.0.5",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "reactiveweb": "^1.4.2",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^36.0.1",
    "tracked-built-ins": "^3.4.0",
    "typescript": "4.7.4",
    "vite": "^6.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  }
}
